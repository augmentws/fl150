{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Suggest Additional Forms",
  "description": "Form for suggesting other questions",
  "type": "object",
  "isSection": false,
  "properties": {
    "suggestion": {
      "label": "Suggestion",
      "type": "object",
      "isSection": true,
      "properties": {
        "question": {
          "type": "string",
